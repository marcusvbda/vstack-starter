{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "activecampaign/api-php": "^2.0",
        "cagartner/correios-consulta": "0.1.*",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.5",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^6.2",
        "laravel/tinker": "^2.0",
        "league/flysystem": "^1.0",
        "league/flysystem-sftp": "^1.0",
        "maatwebsite/excel": "^3.1",
        "marcusvbda/validador-cpf-cnpj": "dev-master",
        "marcusvbda/vstack": "dev-master",
        "multiverse/notazzsdk": "^0.7.0",
        "pagarme/pagarme-php": "^4.0",
        "pusher/pusher-php-server": "^4.1",
        "s-ichikawa/laravel-sendgrid-driver": "2.1",
        "sentry/sentry-laravel": "^1.9",
        "simplesoftwareio/simple-qrcode": "^2.0",
        "slince/shopify-api-php": "^2.5",
        "spatie/async": "^1.5",
        "spatie/laravel-cors": "^1.6",
        "spatie/laravel-permission": "^3.11",
        "tholu/php-packer": "^0.2.0"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/marcusvbda/vstack.git"
        },
        {
            "type": "git",
            "url": "https://github.com/marcusvbda/validador-cpf-cnpj.git"
        }
    ],
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
